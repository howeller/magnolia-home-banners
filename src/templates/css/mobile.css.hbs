.container {
	position: absolute;
}

.txt-wrapper { width: {{size.defaultPicX}}; }

/* Text */

p {
	text-align: center;
	width: 100%;
	line-height: 1;
}

/* Images */

img.pic {
	top: 0;
	right: 0;
	object-fit: contain;
	height: 50px;
}

div.line {
	background-color: black;
	height: 1px;
}
{{#each frame_modules}}

{{!--/* Module {{int @index}} Start : {{this.css}} */--}}
{{> (getName this.css) root=..}}
{{!--/* Module {{int @index}} End  : {{this.css}} */--}}
{{/each}}
